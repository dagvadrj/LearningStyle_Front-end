{
  "expo": {
    "name": "frontend1",
    "slug": "frontend1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/nexoraLogoWhite.png",
    "userInterfaceStyle": "light",
    "statusBar": {
      "style": "dark"
    },
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/nexoraLogoWhite.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow this app to access your microphone"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#ffffff",
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets.",
        "NSMicrophoneUsageDescription": "This app needs microphone access for audio playback.",
        "UIBackgroundModes": ["audio"],
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.bssab.learningStyle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://assets.withfra.me/SignIn.2.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "https://assets.withfra.me/SignIn.2.png"
    },
    "expo-media-library": {
      "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
      "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos."
    },
    "extra": {
      "eas": {
        "projectId": "b03d524a-cf54-4b00-b710-d8e154b8b2aa"
      }
    }
  }
}
